{
  "name": "afriquadis-bilan-express",
  "version": "1.0.0",
  "description": "Application web d'autodiagnostic AFRIQUADIS - Bilan Express - Version Production",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next build && next export",
    "deploy": "npm run export && gh-pages -d out",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "clean": "if exist .next rmdir /s /q .next && if exist out rmdir /s /q out",
    "analyze": "ANALYZE=true npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "production:build": "npm run clean && npm run build",
    "production:start": "set NODE_ENV=production && npm run start",
    "docker:build": "docker build -t afriquadis-bilan-express .",
    "docker:run": "docker run -p 3000:3000 afriquadis-bilan-express",
    "icons:gen": "node scripts/generate-icons.mjs"
  },
